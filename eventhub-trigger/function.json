{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "event",
      "direction": "in",
      "eventHubName": "sec-eventHub1",
      "connection": "receiverConnectionString",
      "cardinality": "many",
      "consumerGroup": "$Default"
    },
    {
      "type": "sendGrid",
      "name": "outputMessage",
      "from": "sender@contoso.com",
      "apiKey": "SENDGRID_API_KEY",
      "direction": "out"
    }
  ]
}